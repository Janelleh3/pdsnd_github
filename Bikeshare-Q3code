#Explore Bikeshare Data
#Question 3-What does travel time look like across Washington?
install.packages('ggplot2')
library(ggplot2)
#read in data
washington <- read.csv('washington.csv')

#create a box plot for Washington with a focus on trip duration
ggplot(washington, aes(x=factor(0), y=Trip.Duration)) + 
  geom_boxplot(color="purple", fill="yellow")+
  coord_cartesian(ylim = c(0, 1500))+
  #add title and axis labels
  xlab('Washington')+
  ylab('Trip Duration (seconds)')+
  ggtitle('Washington DC Bikeshare Trip Length')

summary(washington)